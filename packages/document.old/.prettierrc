{
  "embeddedLanguageFormatting": "off",
  "printWidth": 120,
  "quoteProps": "consistent",
  "singleQuote": true,
  "trailingComma": "none",
  "importOrder": [
    "^react", 
    "^next", 
    "<THIRD_PARTY_MODULES>", 
    "^@app/(.*)$",
    "^\\$",
    "^[.](.*)"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false
}
