{
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["dom", "es2019"],
    "jsx": "preserve",
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnreachableCode": false
  }
}
